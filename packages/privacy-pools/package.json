{
  "name": "@kohaku-eth/privacy-pools",
  "version": "0.0.1-alpha.1",
  "description": "Simplified Privacy Pools TS utils",
  "type": "module",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "echo Building @kohaku-eth/privacy-pools && tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch",
    "demo": "tsx demo/simple.ts",
    "test": "vitest run",
    "test:e2e": ". ./.env.e2e.tests && vitest --inspect-brk --no-file-parallelism run e2e",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm -w -r build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "ethers": "^6.13.1"
  },
  "peerDependenciesMeta": {
    "ethers": {
      "optional": true
    }
  },
  "dependencies": {
    "@0xbow/privacy-pools-core-sdk": "^1.0.2",
    "@fatsolutions/privacy-pools-core-circuits": "1.0.3",
    "@kohaku-eth/plugins": "workspace:*",
    "@kohaku-eth/provider": "workspace:*",
    "@reduxjs/toolkit": "^2.11.2",
    "maci-crypto": "2.5.0"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "dotenv": "^17.2.1",
    "prool": "^0.0.24",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.17.0",
    "typescript": "^5.9.2",
    "viem": "^2.37.9",
    "vitest": "^3.0.0"
  }
}
